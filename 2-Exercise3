import numpy as np
def q1():
 arr= np.arange(1,13).reshape(1,12)
 narr=arr.reshape(3,4)
 print(narr) 
def q2():
    arr=np.arange(1,37).reshape(6,6)
    narr=arr.reshape(3,12)
    print(narr)
    nnarr=narr.reshape(2,18)
    print(nnarr)
def q3():
    arr=np.arange(1,5).reshape(2,2)
    narr=arr.reshape(1,4)
    print(narr)
def q4():
     arr=np.array([1,2,3])
     col=arr.reshape(-1,1)
     print(col)
     row=arr.reshape(1,-1)
     print(row)
q1()
q2()
q3()
q4()
